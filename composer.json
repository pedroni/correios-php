{
    "name": "pedroni/correios-php",
    "type": "library",
    "description": "Uma maneira f√°cil de interagir com as principais funcionalidades dos Correios.",
    "keywords": [
        "pedroni",
        "correios-php",
        "correios",
        "frete",
        "calculo",
        "rastreio",
        "cep"
    ],
    "homepage": "https://github.com/pedroni/correios-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Pires",
            "email": "lucas.pires.mattos@gmail.com",
            "homepage": "https://github.com/pedroni",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "squizlabs/php_codesniffer": "~3.5"
    },
    "autoload": {
        "psr-4": {
            "Pedroni\\Correios\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pedroni\\Correios\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src"
    },
    "extra": {

    },
    "config": {
        "sort-packages": true
    }
}
